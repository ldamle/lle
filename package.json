{
  "name": "ldamle",
  "version": "1.0.16",
  "description": "Library for drawning and modeling logical elements",
  "main": "dist/lle.js",
  "types": "dist/lle.d.ts",
  "scripts": {
    "test": "bin/testlib&&jest --config ./test/jest.config.js",
    "posttest": "cp ./node_modules/nyc-dark/*.css ./test/coverage/all",
    "test:awotl": "jest --config ./test/jest.awotl.config.js",
    "posttest:awotl": "cp ./node_modules/nyc-dark/*.css ./test/coverage/awotl",
    "test:testlib": "bin/testlib&&jest --config ./test/jest.testlib.config.js",
    "test:element": "jest --config ./test/jest.element.config.js",
    "posttest:element": "cp ./node_modules/nyc-dark/*.css ./test/coverage/element",
    "test:elementgraph": "jest --config ./test/jest.elementgraph.config.js",
    "posttest:elementgraph": "cp ./node_modules/nyc-dark/*.css ./test/coverage/elementgraph",
    "test:connection": "jest --config ./test/jest.connection.config.js",
    "posttest:connection": "cp ./node_modules/nyc-dark/*.css ./test/coverage/connection/",
    "test:general": "jest --config ./test/jest.general.config.js",
    "posttest:general": "cp ./node_modules/nyc-dark/*.css ./test/coverage/general/",
    "build": "webpack --config webpack.config.js",
    "dev": "tsc --watch",
    "prettier": "prettier --write ./src/**/*.ts ./bin/**/*.ts ./test/**/*.ts ./examples/**/*.ts ./docs/**/*.ts",
    "tslint": "tslint -p tslint -p tsconfig.json",
    "codestyle": "npm run prettier && npm run tslint",
    "docsify": "docsify serve ./docs",
    "typedoc": "typedoc",
    "docs": "npm run typedoc && npm run docsify"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run codestyle"
    }
  },
  "author": "pluttan",
  "license": "MIT",
  "devDependencies": {
    "@types/babel__traverse": "7.0.6",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.7.5",
    "docsify-cli": "^4.3.0",
    "husky": "^3.0.5",
    "jest": "^24.9.0",
    "jest-html-reporters": "^3.1.7",
    "nyc": "^17.1.0",
    "nyc-dark": "^3.0.3",
    "prettier": "^1.18.2",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "string-replace-loader": "^3.1.0",
    "ts-jest": "^24.3.0",
    "ts-node": "^10.8.1",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.26.7",
    "typescript": "5.5.x",
    "webpack-cli": "^5.1.4"
  },
  "overrides": {
    "@types/babel__traverse": "7.0.6"
  },
  "dependencies": {
    "ts-loader": "^9.5.1",
    "typedoc-material-theme": "^1.1.0"
  }
}
