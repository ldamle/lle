{
  "name": "ldamle",
  "version": "1.0.11",
  "description": "Library for drawning and modeling logical elements",
  "main": "dist/lle.js",
  "types": "dist/lle.d.ts",
  "scripts": {
    "test": "jest --config ./test/jest.config.js",
    "test:testlib": "jest --config ./test/jest.testlib.config.js",
    "test:element": "jest --config ./test/jest.element.config.js",
    "build": "webpack --config webpack.config.js",
    "dev": "tsc --watch",
    "prettier": "prettier --write src/* test/* example/*",
    "tslint": "tslint -p tslint -p tsconfig.json",
    "codestyle": "npm run prettier && npm run tslint",
    "docsify": "docsify serve ./docs",
    "typedoc": "typedoc --out ./docs/api ./src --mode file --readme docs/README.md",
    "docs": "npm run typedoc && npm run docsify"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run codestyle"
    }
  },
  "author": "pluttan",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^24.9.1",
    "@types/node": "^12.7.5",
    "docsify-cli": "^4.3.0",
    "husky": "^3.0.5",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "string-replace-loader": "^3.1.0",
    "ts-jest": "^24.3.0",
    "ts-node": "^10.8.1",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.15.0",
    "typescript": "^4.6.3",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "ts-loader": "^9.5.1"
  }
}
